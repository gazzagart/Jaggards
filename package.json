{
  "name": "jaggards",
  "productName": "Jaggards",
  "version": "0.5.5",
  "description": "The Jaggards Desktop app to handle .csv and Excel data",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src"
  },
  "keywords": [],
  "author": "gazza",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "./src/JaggardsIco.ico"
      },
      "electronWinstallerConfig": {
        "name": "Jaggards",
        "authors": "Maybery Dev Team",
        "title": "Jaggards Desktop Application",
        "loadingGif": "./src/loading.gif",
        "setupIcon": "./src/JaggardsIco.ico",
        "iconUrl": "https://raw.githubusercontent.com/gazzagart/Jaggards/master/src/JaggardsIco.ico",
        "noMsi": true,
        "setupExe": "Jaggards Setup.exe"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "Gareth Maybery",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Jaggards"
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
    "electron-compile": "6.4.4",
    "electron-devtools-installer": "2.2.4",
    "electron-squirrel-startup": "1.0.0",
    "exceljs": "^1.15.0",
    "hoek": "^6.1.3",
    "papaparse": "^5.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "4.12.12",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-css": "^4.2.1",
    "constantinople": "^3.1.2",
    "cryptiles": "^4.1.3",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "electron-winstaller": "^4.0.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "mime": "^2.4.4",
    "minimatch": "^3.0.4",
    "uglify-js": "^3.6.0"
  }
}
